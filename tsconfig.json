{
  "compilerOptions": {
    "outDir": "dist",
    "typeRoots": ["./src/@types", "./node_modules/@types"],
    "target": "es2016",
    "declaration": false,
    "module": "commonjs",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "strictNullChecks": false
  }
}